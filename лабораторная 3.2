#include <iostream>
using namespace std;
int factorial(int N)
{
	int F = 1;	
	for(int i=1; i<=N; ++i)
  {
		F *= i; // вычисление факториала
	}
	return F;
}
int main()
{
	int N;
	cout << "N = ";
	cin >> N;	
	if(N >= 0) // обязательное условие 
	{
		cout << factorial(N); 
	}
	else
	{
		cout << "Ошибка: N < 0.";
	}	
	return 0;
}
